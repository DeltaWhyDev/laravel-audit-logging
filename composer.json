{
    "name": "deltawhydev/laravel-audit-logging",
    "description": "Unified audit logging system for Laravel with Nova support",
    "keywords": [
        "laravel",
        "audit",
        "logging",
        "nova",
        "changelog"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DeltaWhyDev",
            "email": "me@deltawhy.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0"
    },
    "suggest": {
        "laravel/nova": "Required to use the Nova Audit Log resource and visualizer."
    },
    "autoload": {
        "psr-4": {
            "DeltaWhyDev\\AuditLog\\": "src/"
        },
        "files": [
            "src/Helpers/AuditLogHelper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeltaWhyDev\\AuditLog\\Providers\\AuditLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
